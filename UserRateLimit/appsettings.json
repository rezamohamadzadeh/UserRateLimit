{
  "ConnectionStrings": {
    "RateLimit": "Data Source=RateLimit.db"
  },
  "RateLimitDateTime": "10", // minute
  "CronExpression": {
    "everySecound": "* * * ? * *",
    "everyMinute": "0 * * ? * *",
    "every10Minute": "0 */10 * ? * *"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
